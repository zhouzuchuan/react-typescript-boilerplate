{ 
    "extends": ["stylelint-config-standard", "stylelint-config-recess-order"],
    "rules": {
        "at-rule-no-unknown": [true, { "ignoreAtRules": ["mixin", "extend", "content"] }],
        "no-missing-end-of-source-newline": null,
        "indentation": 4,
        "declaration-empty-line-before": "never"
    }
}
