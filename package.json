{
    "name": "react-typescript-boilerplate",
    "version": "2.0.0",
    "private": true,
    "group": "zhouzuchuan",
    "description": "React typescript boilerplate based on secondary development of create-react-app",
    "author": {
        "name": "fezzc",
        "email": "zhouzuchuan@sina.com"
    },
    "dependencies": {
        "@date-io/dayjs": "^2.13.1",
        "@dseven/hooks": "^0.1.4",
        "@dseven/utils": "^0.0.1",
        "@emotion/react": "^11.9.0",
        "@emotion/styled": "^11.8.1",
        "@mui/icons-material": "^5.6.0",
        "@mui/lab": "^5.0.0-alpha.76",
        "@mui/material": "^5.6.0",
        "@mui/styled-engine-sc": "^5.5.2",
        "@testing-library/jest-dom": "^5.16.4",
        "@testing-library/react": "^12.1.4",
        "@testing-library/user-event": "^13.5.0",
        "@types/jest": "^27.4.1",
        "@types/node": "^16.11.26",
        "@types/react": "^17.0.43",
        "@types/react-dom": "^17.0.14",
        "api-manage": "^2.3.0",
        "axios": "^0.26.1",
        "classnames": "^2.3.1",
        "css.preset": "^0.2.0",
        "dayjs": "^1.11.0",
        "immer": "^9.0.12",
        "js-cookie": "^3.0.1",
        "lodash": "^4.17.21",
        "normalize.css": "^8.0.1",
        "notistack": "^2.0.4",
        "react": "^18.0.0",
        "react-dom": "^18.0.0",
        "react-enhanced": "^3.0.1",
        "react-router-dom": "^6.3.0",
        "react-scripts": "5.0.0",
        "recoil": "^0.7.0",
        "styled-components": "^5.3.5",
        "typescript": "^4.6.3",
        "web-vitals": "^2.1.4"
    },
    "scripts": {
        "start": "react-app-rewired start",
        "build": "react-app-rewired build",
        "test": "react-app-rewired test",
        "eject": "react-scripts eject",
        "analyze": "cross-env source_map=true react-app-rewired build &&  source-map-explorer 'build/static/js/*.js'",
        "lint:css": "stylelint 'src/**/*.tsx' --fix",
        "lint:js": "eslint 'src/**/*.tsx' 'src/**/*.ts' 'src/**/*.js'",
        "lint": "npm run lint:js && npm run lint:css",
        "commit": "git-cz",
        "release": "standard-version",
        "prepare": "husky install",
        "postinstall": "npx husky install"
    },
    "config": {
        "commitizen": {
            "path": "node_modules/cz-customizable"
        }
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "commit-msg": "commitlint -e $GIT_PARAMS"
        }
    },
    "lint-staged": {
        "src/**/*.{ts,tsx,less,css,json}": [
            "prettier --write",
            "npm run lint",
            "git add ."
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@commitlint/cli": "^16.2.3",
        "@commitlint/config-conventional": "^16.2.1",
        "@stylelint/postcss-css-in-js": "^0.37.2",
        "@types/classnames": "^2.3.1",
        "@types/js-cookie": "^3.0.1",
        "@types/lodash": "^4.14.181",
        "@types/react-router-dom": "^5.3.3",
        "@types/styled-components": "^5.1.24",
        "@types/webpack-env": "^1.16.3",
        "babel-plugin-import": "^1.13.3",
        "commitizen": "^4.2.4",
        "cross-env": "^7.0.3",
        "customize-cra": "^1.0.0",
        "cz-customizable": "^6.3.0",
        "data-mock": "^2.1.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.0.0",
        "html-webpack-comment-plugin": "^0.0.2",
        "husky": "^7.0.4",
        "less": "^4.1.2",
        "less-loader": "^10.2.0",
        "lint-staged": "^12.3.7",
        "lodash-webpack-plugin": "^0.11.6",
        "postcss-syntax": "^0.36.2",
        "prettier": "^2.6.2",
        "react-app-rewired": "^2.2.1",
        "source-map-explorer": "^2.5.2",
        "standard-version": "^9.3.2",
        "stylelint": "^14.6.1",
        "stylelint-config-prettier": "^9.0.3",
        "stylelint-config-recess-order": "^3.0.0",
        "stylelint-config-standard": "^25.0.0",
        "stylelint-config-standard-scss": "^3.0.0",
        "stylelint-config-styled-components": "^0.1.1",
        "stylelint-order": "^5.0.0",
        "stylelint-webpack-plugin": "^3.2.0",
        "webpackbar": "^5.0.2"
    }
}
